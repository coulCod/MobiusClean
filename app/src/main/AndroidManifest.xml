<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.matsyuk.mobiusclean">

    <application
        android:name=".clean.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".clean.ui.main.views.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".clean.ui.first_wizard.views.FirstActivity">
        </activity>

        <activity android:name=".clean.ui.app_lock.views.AppLockActivity">
        </activity>

        <activity android:name=".clean.ui.first_wizard.views.StartLoginActivity">
        </activity>
    </application>

</manifest>